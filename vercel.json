{
    "version": 2,
    "builds": [
      {
        "src": "api/server.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["api/db.json"]
        }
      }
    ],
    "routes": [
      {
        "source": "/api/(.*)",
        "destination": "api/server.js"
      }
    ]
  } 